    <Assign DisplayName="Write PowerShell script to get folder sizes from specified location" sap:VirtualizedContainerService.HintSize="434,115" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="x:String">[PowerShellScript]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:String" xml:space="preserve"># Define the path as input
Param(
    [string]$Path,
    [int]$MaxFolders = 10
)  # Replace this with the desired folder path

# Get the top 10 folders by size in GB
$Result = Get-ChildItem -Path $Path -Directory | 
ForEach-Object {
    [PSCustomObject]@{
        Name = $_.Name
        FullPath = $_.FullName
        SizeGB = [math]::Round((Get-ChildItem -Path $_.FullName -Recurse -File | Measure-Object -Property Length -Sum).Sum / 1GB, 2)
    }
} | Sort-Object -Property SizeGB -Descending | Select-Object -First 10

# Convert the result to JSON format
$Result | ConvertTo-Json -Depth 3</InArgument>
      </Assign.Value>
    </Assign>
